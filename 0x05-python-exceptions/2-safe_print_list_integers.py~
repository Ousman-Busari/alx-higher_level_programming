#!/usr/bin/python3

def safe_print_list_integers(my_list=[], x=0):

    print_count = 0

    for i in range(x):
        try:
            print("{:d}".format(my_list[i]))
        except (ValueError, IndexError):
            break
        else:
            print_count += 1

    print()
    return(print_count)
